class Solution:
    def maxSubseq(self, s, k):
        stack = []

        for c in s:
            while stack and k > 0 and stack[-1] < c:
                stack.pop()
                k -= 1
            stack.append(c)

        
        while k > 0 and stack:
            stack.pop()
            k -= 1

        return ''.join(stack)
