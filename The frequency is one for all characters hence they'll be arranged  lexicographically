class Solution:
    def frequencySort(self, s):
        
        freq = {}
        for ch in s:
            freq[ch] = freq.get(ch, 0) + 1

        
        sorted_chars = sorted(freq.items(), key=lambda x: (x[1], x[0]))

        
        result = ""
        for ch, f in sorted_chars:
            result += ch * f

        return result


